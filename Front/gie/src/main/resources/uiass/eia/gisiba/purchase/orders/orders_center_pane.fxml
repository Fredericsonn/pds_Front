<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="centerAnchorPane" prefHeight="541.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="searchBtn" layoutX="117.0" layoutY="84.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="86.0" text="SEARCH" />
        <Button fx:id="createNewProductBtn" layoutX="289.0" layoutY="84.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="86.0" text="CREATE NEW" />
      <TableView fx:id="ordersTableView" layoutY="129.0" prefHeight="414.0" prefWidth="530.0" />
      <HBox fx:id="mainHbox" alignment="CENTER" prefHeight="82.0" prefWidth="530.0" spacing="40.0">
         <children>
            <VBox fx:id="categoryVbox" prefHeight="94.0" prefWidth="130.0" spacing="5.0">
               <children>
                  <Label text="Category :" />
                  <ComboBox fx:id="categoryComboBox" prefHeight="34.0" prefWidth="130.0" promptText="category" />
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
            <VBox fx:id="brandVbox" prefHeight="130.0" prefWidth="130.0" spacing="5.0">
               <children>
                  <Label text="Brand :" />
                  <ComboBox fx:id="brandComboBox" prefHeight="34.0" prefWidth="130.0" promptText="brand" />
               </children>
            </VBox>
            <VBox fx:id="modelVbox" prefHeight="130.0" prefWidth="130.0" spacing="5.0">
               <children>
                  <Label text="Model :" />
                  <ComboBox fx:id="modelComboBox" prefHeight="34.0" prefWidth="130.0" promptText="model" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets top="15.0" />
         </padding>
      </HBox>
      <AnchorPane fx:id="refreshPane" layoutX="477.0" layoutY="87.0" prefHeight="40.0" prefWidth="43.0">
         <children>
            <ImageView fx:id="refreshImg" fitHeight="32.0" fitWidth="32.0" layoutX="6.0" layoutY="4.0">
               <image>
                  <Image url="@../../imgs/refresh.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
